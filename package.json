{
  "name": "@cosmic-plus/i18n",
  "version": "1.2.0",
  "description": "Lightweight i18n implementation that works both in Node.js and browser",
  "author": "MisterTicot",
  "homepage": "https://cosmic.plus/#view:js-i18n",
  "repository": "github:cosmic-plus/js-i18n",
  "license": "MIT",
  "main": "src/index.js",
  "module": "es5/index.js",
  "keywords": [
    "i18n",
    "translation",
    "lightweight",
    "node",
    "browser"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier --write --no-semi 'src/*.js' '*.js' '*.json' '*.md'",
    "eslint": "eslint --fix 'src/*.js' '*.js'",
    "lint": "npm run prettier && npm run eslint",
    "build": "babel --delete-dir-on-start --out-dir es5 src",
    "watch": "babel --watch --delete-dir-on-start --out-dir es5 src"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "eslint": "^5.16.0",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@cosmic-plus/jsutils": "^2.0.2",
    "y18n": "^4.0.1",
    "y18n-browser": "^1.0.2"
  }
}
